#Location of NSUI Host
NSUI_HOST_URL=http://localhost:8000

#When "true", app code will be sourced locally from http://localhost:8001
USE_LOCAL_NSUI_APP=true

#Indicate which browser is going to be used. Only "chrome" or "firefox" are supported.
BROWSER=chrome

#Indicates if the Driver will go headless
DRIVE_MODE_HEADLESS=true
